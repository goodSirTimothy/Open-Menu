<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.settings.CreateHallways">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="10.0">
               <children>
                  <Label text="Number of Hallways" />
                  <ChoiceBox fx:id="hallDropBox" onAction="#dropBox" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
               </padding>
            </HBox>
            <Label fx:id="tvError">
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#submitClicked" text="Submit" />
            <ImageView fitHeight="18.0" fitWidth="117.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#resetClicked" text="Reset" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <ScrollPane BorderPane.alignment="CENTER">
         <content>
            <VBox>
               <children>
                  <HBox spacing="10.0">
                     <children>
                        <ImageView fitHeight="14.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true" />
                        <Label text="Hallway" />
                        <ImageView fitHeight="13.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" />
                        <Label text="Lowest Room Number" />
                        <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true" />
                        <Label text="Highest Room Number" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall1" spacing="10.0">
                     <children>
                        <Label text="Hall 1" />
                        <TextField fx:id="etHall1" />
                        <TextField fx:id="hall1LowNum" />
                        <TextField fx:id="hall1HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall2" spacing="10.0">
                     <children>
                        <Label text="Hall 2" />
                        <TextField fx:id="etHall2" />
                        <TextField fx:id="hall2LowNum" />
                        <TextField fx:id="hall2HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall3" spacing="10.0">
                     <children>
                        <Label text="Hall 3" />
                        <TextField fx:id="etHall3" />
                        <TextField fx:id="hall3LowNum" />
                        <TextField fx:id="hall3HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall4" spacing="10.0">
                     <children>
                        <Label text="Hall 4" />
                        <TextField fx:id="etHall4" />
                        <TextField fx:id="hall4LowNum" />
                        <TextField fx:id="hall4HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall5" spacing="10.0">
                     <children>
                        <Label text="Hall 5" />
                        <TextField fx:id="etHall5" />
                        <TextField fx:id="hall5LowNum" />
                        <TextField fx:id="hall5HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall6" spacing="10.0">
                     <children>
                        <Label text="Hall 6" />
                        <TextField fx:id="etHall6" />
                        <TextField fx:id="hall6LowNum" />
                        <TextField fx:id="hall6HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox fx:id="hall7" spacing="10.0">
                     <children>
                        <Label text="Hall 7" />
                        <TextField fx:id="etHall7" />
                        <TextField fx:id="hall7LowNum" />
                        <TextField fx:id="hall7HighNum" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
